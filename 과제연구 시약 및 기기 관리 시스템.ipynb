    "import re\n"
    "\n",
    "element = re.compile('(\(.*\)[0-9]*)|([A-Z][a-z]?)([0-9]*)')\n",
    "bracket = re.compile('(?<=\().*(?=\))')\n",
    "weight = re.compile('(?<=\))\d+')\n",
    "\n",
    "def chamf(m, w=1, i_dict={}):\n",
    "    result = i_dict\n",
    "    f = element.findall(m)\n",
    "    for t in f:\n",
    "        if t[0] == '':\n",
    "            value = 0\n",
    "            if t[2] == '':\n",
    "                value = w\n",
    "            else:\n",
    "                value = w * int(t[2])\n",
    "            if t[1] not in result:\n",
    "                result[t[1]] = value\n",
    "            else:\n",
    "                result[t[1]] += value\n",
    "        else:\n",
    "            new_in = bracket.findall(t[0])[0]\n",
    "            wei = weight.findall(t[0])\n",
    "            chamf(new_in, int(wei[-1]), result)\n",
    "    return result"
   ]
