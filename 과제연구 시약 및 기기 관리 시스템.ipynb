    "class Queue:\n",
    "\n",
    "    def __init__(self):\n",
    "        self.items = []\n",
    "\n",
    "    def isEmpty(self):\n",
    "        return self.items == []\n",
    "\n",
    "    def enqueue(self, item):\n",
    "        self.items.insert(0,item)\n",
    "\n",
    "    def dequeue(self):\n",
    "        \n",
    "        if len(self.items) > 0 :\n",
    "            return self.items.pop()\n",
    "\n",
    "        else : \n",
    "            return ''\n",
    "\n",
    "    def size(self):\n",
    "        return len(self.items)\n",
    "    \n",
    "    def peek(self) :\n",
    "        \n",
    "        if len(self.items) > 0 :\n",
    "            return self.items[-1]\n",
    "\n",
    "        \n",
    "        else : \n",
    "            return '*'\n",
    "\n",
    "    def peek1(self) :\n",
    "        \n",
    "        if len(self.items) > 1 :\n",
    "            return self.items[-2]\n",
    "\n",
    "        else : \n",
    "            return '*'"
   ]
